{
  "build": {
    "core": "arduino",
    "cpu": "cortex-m0plus",
    "extra_flags": "-D ARDUINO_RASPBERRY_PI_PICO -DARDUINO_ARCH_RP2040",
    "f_cpu": "133000000L",
    "hwids": [
      [
        "0x2886",
        "0x002f"
      ]
    ],
    "mcu": "rp2040",
    "variant": "RASPBERRY_PI_PICO"
  },
  "debug": {
    "jlink_device": "RP2040_M0_0",
