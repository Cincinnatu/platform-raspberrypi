    "require_upload_port": true,
    "native_usb": true,
    "use_1200bps_touch": false,
    "wait_for_upload_port": true,
    "protocol": "picotool",
    "protocols": [
      "cmsis-dap",
      "jlink",
      "raspberrypi-swd",
      "picotool"
    ]
